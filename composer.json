{
    "name": "tepuilabs/laravel-instapago",
    "description": "Library for payments in VEF (Bsf.) in Venezuela.",
    "keywords": [
        "Tepuilabs",
        "laravel",
        "laravel-instapago"
    ],
    "homepage": "https://github.com/tepuilabs/laravel-instapago",
    "license": "MIT",
    "authors": [
        {
            "name": "Angel Cruz",
            "email": "angel@tepuilabs.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "instapago/instapago": "^7.0",
        "spatie/laravel-package-tools": "^1.13.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Tepuilabs\\LaravelInstapago\\": "src",
            "Tepuilabs\\LaravelInstapago\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tepuilabs\\LaravelInstapago\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tepuilabs\\LaravelInstapago\\LaravelInstapagoServiceProvider"
            ],
            "aliases": {
                "LaravelInstapago": "Tepuilabs\\LaravelInstapago\\Facades\\LaravelInstapago"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
